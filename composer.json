{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.1",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "maatwebsite/excel": "*",
        "nwidart/laravel-modules": "^12.0",
        "paypal/paypal-checkout-sdk": "^1.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.20",
        "stancl/tenancy": "^3.9",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
  "autoload": {
  "psr-4": {
    "App\\": "app/",
    "Database\\Factories\\": "database/factories/",
    "Database\\Seeders\\": "database/seeders/",
    "Modules\\": "Modules/",
    "Modules\\Alumni\\": "Modules/Alumni/app/",
    "Modules\\Cafeteria\\": "Modules/Cafeteria/app/",
    "Modules\\Academic\\": "Modules/Academic/app/",
    "Modules\\Analytics\\": "Modules/Analytics/app/",
    "Modules\\Assets\\": "Modules/Assets/app/",
    "Modules\\Attendance\\": "Modules/Attendance/app/",
    "Modules\\Audit\\": "Modules/Audit/app/",
    "Modules\\Communication\\": "Modules/Communication/app/",
    "Modules\\Compliance\\": "Modules/Compliance/app/",
    "Modules\\Core\\": "Modules/Core/app/",
    "Modules\\Document\\": "Modules/Document/app/",
    "Modules\\ELibrary\\": "Modules/ELibrary/app/",
    "Modules\\Examination\\": "Modules/Examination/app/",
    "Modules\\Finance\\": "Modules/Finance/app/",
    "Modules\\Hostel\\": "Modules/Hostel/app/",
    "Modules\\HR\\": "Modules/HR/app/",
    "Modules\\Inventory\\": "Modules/Inventory/app/",
    "Modules\\Library\\": "Modules/Library/app/",
    "Modules\\LMS\\": "Modules/LMS/app/",
    "Modules\\Localization\\": "Modules/Localization/app/",
    "Modules\\Marketplace\\": "Modules/Marketplace/app/",
    "Modules\\Notification\\": "Modules/Notification/app/",
    "Modules\\Portal\\": "Modules/Portal/app/",
    "Modules\\PWA\\": "Modules/PWA/app/",
    "Modules\\Research\\": "Modules/Research/app/",
    "Modules\\Settings\\": "Modules/Settings/app/",
    "Modules\\Timetable\\": "Modules/Timetable/app/",
    "Modules\\Transport\\": "Modules/Transport/app/",
    "Modules\\Welfare\\": "Modules/Welfare/app/",
    "Modules\\ChatBot\\": "Modules/ChatBot/app/"
  }

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
